<h1>User info</h1>
<!-- form_with部分 -->
<%= form_with model:@user, local: true do |f| %>
  <h4>Name</h4>
  <%= f.text_field :name %>
  <%= attachment_image_tag @user, :image, :fill, 60, 60 %>
  <label for="inputImage">Image</label>
  <%= f.attachment_field :image, placeholder: "image" %>
  <h4>introduction</h4>
  <%= f.text_area :introduction %>
  <%= f.submit 'Update User' %>
<% end %>